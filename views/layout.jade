								h4 to Celebrate our Wedding
								div(class="heartline")
									hr
									i(class="fa fa-heart")
								h4 June 7th, 2015
								p(class="downArrow")
									a(href="")
										i(class="fa fa-chevron-down")
						img(src="/images/amyjon1.jpg")

