section(class="grey_section section_gap", id="weddingParty")
	div(class="container")
		div(class="row heading")
			div(class="col-xs-12 col-sm-12 col-md-5 col-lg-5 noPadd")
				h2 The Wedding Party

		div(class="row bride")
			ul(class="clearfix")
				li(class="col-xs-12 col-sm-4 col-md-3 col-lg-3 pinkCircle")
					div(class="nameCircle")
						em Brides Maids
				li(class="col-xs-12 col-sm-2 col-md-3 col-lg-3 circle")
					img(src="images/miranda.jpg", class="smallRound")
					h4 Miranda Yarbrough
						span Maid of Honor
				li(class="col-xs-12 col-sm-2 col-md-3 col-lg-3 circle")
					img(src="images/brooke.jpg", class="smallRound")
					h4 Brooke Sunwold
						span Bridesmaid
				li(class="col-xs-12 col-sm-2 col-md-3 col-lg-3 circle")
					img(src="images/jo.jpg", class="smallRound")
					h4 Jo Holland
						span Bridesmaid

		div(class="row groom")
			ul(class="clearfix")
				li(class="col-xs-12 col-sm-4 col-md-3 col-lg-3 blueCircle")
					div(class="nameCircle")
						em Grooms Men
				li(class="col-xs-12 col-sm-2 col-md-3 col-lg-3 circle")
					img(src="images/corey.jpg", class="smallRound")
					h4 Corey Sunwold
						span Best Man
				li(class="col-xs-12 col-sm-2 col-md-3 col-lg-3 circle")
					img(src="images/chad.jpg", class="smallRound")
					h4 Chad Moran
						span Groomsman
				li(class="col-xs-12 col-sm-2 col-md-3 col-lg-3 circle")
					img(src="images/paolo.jpg", class="smallRound")
					h4 Paolo Ciccu
						span Groomsman